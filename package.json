    "parcel": "^2.11.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "formik": "^2.4.5",
    "gas-client": "^1.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "yup": "^1.3.3"
